<h1>Listing landing_pages</h1>

<table>
  <thead>
    <tr>
      <th>Headline</th>
      <th>Subhead</th>
      <th>Image</th>
      <th>Captionbutton text</th>
      <th>Call to action</th>
      <th>Button color</th>
      <th>Background color</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @landing_pages.each do |landing_page| %>
      <tr>
        <td><%= landing_page.headline %></td>
        <td><%= landing_page.subhead %></td>
        <td><%= landing_page.image %></td>
        <td><%= landing_page.captionbutton_text %></td>
        <td><%= landing_page.call_to_action %></td>
        <td><%= landing_page.button_color %></td>
        <td><%= landing_page.background_color %></td>
        <td><%= link_to 'Show', landing_page %></td>
        <td><%= link_to 'Edit', edit_landing_page_path(landing_page) %></td>
        <td><%= link_to 'Destroy', landing_page, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Landing page', new_landing_page_path %>
